{
  "version": 3,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/chapters/(.*)",
      "dest": "/src/index.js"
    },
    {
      "src": "/api/dictionary/(.*)",
      "dest": "/src/index.js"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
  ]
}
